{
	"version": 2,
	"builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
		],
	"routes": [
		{ "src": "/(.*)", 
		"dest": "dist/index.js",
		 

	"headers": {
    "Access-Control-Allow-Origin": "*"
  }
	}
	],
	

	"outputDirectory": "dist"
}