{
	"version": 2,
	"builds": [
		{ "src": "/server.ts", "use": "@vercel/node" }
	],
	"routes": [
		{ "src": "/(.*)", 
		"dest": "/server.ts", 
		 

	"headers": {
    "Access-Control-Allow-Origin": "*",
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
    "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
  }
	}
	],
	

	"outputDirectory": "build"
}