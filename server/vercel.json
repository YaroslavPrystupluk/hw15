{
	"version": 2,
	"builds": [
		{ "src": "./build/server.js", "use": "@vercel/node" }
	],
	"routes": [
		{ "src": "/(.*)", "dest": "/build/server.js", 

	"headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
	}
	],
	

	"outputDirectory": "build"
}